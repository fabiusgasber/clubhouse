<article>
        <div>
            <h2><%= message.title %></h2>
            <p><%= message.text %></p>
        </div>
        <div class="message-meta">
            <p><%= message.first_name %></p>
            <span><%= new Date(message.timestamp).toLocaleString() %></span>
        </div>
            <a href="/messages/delete/<%= message.id %>"><img src="/trash-can.png" alt="trash can" width="25px" height="25px"></a>
</article>